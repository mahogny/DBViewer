  <brick id="728801837">
    <name>SFP</name>
    <abstract />
    <long_description />
    <notes />
    <license />
    <media />
    <assembly_instruction>
      <step>
        <description>Get Koruza SFP extension PCB panel consisting of two larger SFP socket boards and three smaller SFP plug boards.</description>
        <media>
          <file url="./././usdata/sfp1.jpg" />
        </media>
        <component quantity="1" id="1043618185" />
      </step>
      <step>
        <description>Solder the SFP connector to the boards, make sure it is oriented correctly, with the notch away from the rest of connectors.</description>
        <media>
          <file url="./././usdata/sfp2.jpg" />
        </media>
      </step>
      <step>
        <description>Solder the SATA connector to SFP socket boards...</description>
        <media>
          <file url="./././usdata/sfp3.jpg" />
        </media>
      </step>
      <step>
        <description>...make sure to solder the mounting legs as well.</description>
        <media>
          <file url="./././usdata/sfp4.jpg" />
        </media>
      </step>
      <step>
        <description>Solder the SFP cage to SFP socket boards.</description>
        <media>
          <file url="./././usdata/sfp5.jpg" />
        </media>
      </step>
      <step>
        <description>Solder IDC10 sockets to the boards, ckeck their orientation before soldering!</description>
        <media>
          <file url="./././usdata/sfp6.jpg" />
        </media>
      </step>
      <step>
        <description>Prepare the 3D printed sleeve for the SFP extension plug.</description>
        <media>
          <file url="./././usdata/sfp7.jpg" />
        </media>
      </step>
      <step>
        <description>Connect the SFP socket and plug board with SATA cable.</description>
        <media>
          <file url="./././usdata/sfp8.jpg" />
        </media>
      </step>
      <step>
        <description>Assemble 15cm long IDC 10 cable.</description>
        <media>
          <file url="./././usdata/sfp9.jpg" />
        </media>
      </step>
      <step>
        <description>Connect SFP socket and plug boards with the IDC cable.</description>
        <media>
          <file url="./././usdata/sfp10.jpg" />
        </media>
      </step>
    </assembly_instruction>
