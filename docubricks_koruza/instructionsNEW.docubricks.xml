  <brick id="868260495">
    <name>Mounting</name>
    <abstract />
    <long_description />
    <notes />
    <license />
    <media />
    <assembly_instruction>
      <step>
        <description>Take the 3D printed inner ring.</description>
        <media>
          <file url="./././usdata/3d3.JPG" />
        </media>
      </step>
      <step>
        <description>Insert four M4 square nuts into the provided slots. Help yourself with plaiers to insert the nuts.</description>
        <media>
          <file url="./././usdata/mounting1.JPG" />
        </media>
        <component quantity="1" id="631908533" />
        <component quantity="1" id="1715079572" />
      </step>
      <step>
        <description>Push the nuts all the way in with the screwdriver.</description>
        <media>
          <file url="./././usdata/mounting2.JPG" />
        </media>
      </step>
      <step>
        <description>Take the 3D printed outer ring.</description>
        <media>
          <file url="./././usdata/mounting4.JPG" />
          <file url="./././usdata/3d2.JPG" />
        </media>
      </step>
      <step>
        <description>Insert 8 35mm M3 screws trough the provided holes, such that screw heads are located on the smooth surface.</description>
        <media>
          <file url="./././usdata/mounting3.JPG" />
        </media>
      </step>
      <step>
        <description>Prepare the custum made rubber.</description>
        <media>
          <file url="./././usdata/rubber.JPG" />
        </media>
      </step>
      <step>
        <description>Put the rubber on the screws. Keep in mind that holes are not symmetrical to preserve the correct orientation.</description>
        <media>
          <file url="./././usdata/0-mounting4.JPG" />
        </media>
      </step>
      <step>
        <description>Place the inner ring on top. Keep in mind the orientation, such that screws are nicely aligned.</description>
        <media>
          <file url="./././usdata/mounting5.JPG" />
        </media>
      </step>
      <step>
        <description>Tighten the M3 screws such that they firmlly hold the rubber, but do not tighten them fully.</description>
        <media>
          <file url="./././usdata/0-mounting5.JPG" />
        </media>
      </step>
      <step>
        <description>Take the laser mount part.</description>
        <media>
          <file url="./././usdata/3d6.JPG" />
        </media>
      </step>
      <step>
        <description>Insert 10 M3 nuts in the laser mount part. Use pliers.</description>
        <media>
          <file url="./././usdata/mounting6.JPG" />
        </media>
      </step>
      <step>
        <description>Insert two M4 nuts.</description>
        <media>
          <file url="./././usdata/mounting7.JPG" />
        </media>
      </step>
      <step>
        <description>Prepare the lens outer ring.</description>
        <media>
          <file url="./././usdata/3d4.JPG" />
        </media>
      </step>
      <step>
        <description>Insert 8 M3 25mm screws trough the lens outer ring, from the side which has holes for screw-heads.</description>
        <media>
          <file url="./././usdata/mounting8.JPG" />
        </media>
      </step>
      <step>
        <description>Place the outer ring trough the rubber, such that the screws are on the same side as ones on the outer ring.</description>
        <media>
          <file url="./././usdata/mounting9.JPG" />
        </media>
      </step>
      <step>
        <description>Take the lens mount part.</description>
        <media>
          <file url="./././usdata/3d5.JPG" />
        </media>
      </step>
      <step>
        <description>Insert the lens with the curve part pointing outwards.</description>
        <media>
          <file url="./././usdata/mounting10.JPG" />
        </media>
      </step>
      <step>
        <description>Insert the lens mounting ring on the screws from the inside, such that the retaining edge is pointing to the rubber.</description>
        <media>
          <file url="./././usdata/mounting11.JPG" />
        </media>
      </step>
      <step>
        <description>Insert the laser pointer such that it sits nicely in the rubber.</description>
        <media>
          <file url="./././usdata/mounting12.JPG" />
        </media>
      </step>
      <step>
        <description>Tread the wires of the laser trough the hole in the laser mount.</description>
        <media>
          <file url="./././usdata/mounting13.JPG" />
        </media>
      </step>
      <step>
        <description>Screw the laser mount part trough the inner ring.</description>
        <media>
          <file url="./././usdata/mounting14.JPG" />
        </media>
      </step>
      <step>
        <description>Take stainless still rods and put them into the holes. If rods do not fit, re-drill the holes with the 8mm drill. Hammer them all the way in.</description>
        <media>
          <file url="./././usdata/mounting15.JPG" />
        </media>
      </step>
      <step>
        <description>Insert the two M4 set screws into the holes and tighten them.</description>
        <media>
          <file url="./././usdata/mounting16.JPG" />
        </media>
      </step>
      <step>
        <description>Take two wedges.</description>
        <media>
          <file url="./././usdata/3d7.JPG" />
        </media>
      </step>
      <step>
        <description>Fit the M3 square nuts inside the provided slots. Optional: use pliers.</description>
        <media>
          <file url="./././usdata/mounting17.JPG" />
        </media>
      </step>
      <step>
        <description>Insert both wedges into the laser mount.</description>
        <media>
          <file url="./././usdata/mounting18.JPG" />
        </media>
      </step>
      <step>
        <description>STEP 27
Take the two M3 25mm screws and fit them trough the wedge.</description>
        <media>
          <file url="./././usdata/mounting19.JPG" />
        </media>
      </step>
      <step>
        <description>Take the spring and fit it between the aiming laser and the provided hole in the laser mount part.</description>
        <media>
          <file url="./././usdata/mounting20.JPG" />
        </media>
      </step>
    </assembly_instruction>
